<p id="notice"><%= notice %></p>

<h1>years</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @years.each do |year| %>
      <% if year.year != 0 %>
        <tr>
          <td><%= year.year %>年生</td>
          <td><%= link_to 'Show', year %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

